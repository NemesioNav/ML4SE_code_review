def fibonacci(n):
    fib_sequence = []
    a, b = 0, 1
    for i in range(n):
        fib_sequence.append(a)
        a, b = b, a + b
    return fib_sequence

# Print the first 10 Fibonacci numbers
print("The first 10 Fibonacci numbers are: ", fibonacci(10))

# Attempt to divide by zero
result = 10 / 0

# Concatenate a string and an integer without conversion
message = "The result is: " + result

# Access an index that is out of range
numbers = [1, 2, 3, 4, 5]
print(numbers[10])
